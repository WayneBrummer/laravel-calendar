{
  "name": "waynebrummer/laravel-calendar",
  "description": "Provides an easy to use wrapper to collect public holidays & other types. + API layer to a calender",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "api",
    "laravel",
    "public holiday",
    "holiday-api",
    "calendar",
    "guzzle"
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.3",
    "spatie/laravel-query-builder": "^2.1",
    "spatie/laravel-json-api-paginate": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.5",
    "orchestra/testbench": "~4.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Pace\\Calendar\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pace\\Calendar\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Pace\\Calendar\\ServiceProvider"
      ]
    }
  }
}
